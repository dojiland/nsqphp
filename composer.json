{
    "name": "per3evere/nsqphp",
    "description": "PHP client for NSQ for Laravel && Lumen",
    "license": "MIT",
    "keywords": [
        "nsq", "lumen", "laravel"
    ],
    "authors": [
        {
            "name": "Persevere Von",
            "email": "wu543065657@163.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "react/event-loop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "suggest": {
        "laravel/framework": "To test the Laravel bindings",
        "laravel/lumen-framework": "To test the lumen bindings"
    },
    "autoload": {
        "psr-4": {
            "Per3evere\\Nsq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Per3evere\\Nsq\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Per3evere\\Nsq\\NsqServiceProvider"
            ]
        }
    }
}
